---
const props = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width="48"
  height="48"
  viewBox="0 0 24 24"
  {...props}
>
  <title>linkedin logo</title>
  <circle cx="4" cy="4" r="2" fill="white" fill-opacity="0">
    <animate fill="freeze" attributeName="fill-opacity" dur="0.6s" values="0;1"
    ></animate>
  </circle>
  <g fill="none" stroke="white" stroke-linecap="round" stroke-width="4">
    <path stroke-dasharray="12" stroke-dashoffset="12" d="M4 10V20">
      <animate
        fill="freeze"
        attributeName="stroke-dashoffset"
        begin="0.3s"
        dur="0.3s"
        values="12;0"></animate>
    </path>
    <path stroke-dasharray="12" stroke-dashoffset="12" d="M10 10V20">
      <animate
        fill="freeze"
        attributeName="stroke-dashoffset"
        begin="0.75s"
        dur="0.3s"
        values="12;0"></animate>
    </path>
    <path
      stroke-dasharray="24"
      stroke-dashoffset="24"
      d="M10 15C10 12.2386 12.2386 10 15 10C17.7614 10 20 12.2386 20 15V20"
    >
      <animate
        fill="freeze"
        attributeName="stroke-dashoffset"
        begin="1.05s"
        dur="0.75s"
        values="24;0"></animate>
    </path>
  </g>
</svg>
